<!DOCTYPE html>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<!--title>Niche Admin - Powerful Bootstrap 4 Dashboard and Admin Template</title-->
<!-- Tell the browser to be responsive to screen width -->
<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

<!-- v4.0.0-alpha.6 -->
<link rel="stylesheet" href="/assets/bootstrap.min.css">

<!-- Google Font -->
<link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

<!-- Theme style -->
<link rel="stylesheet" href="/assets/style.css">
<link rel="stylesheet" href="/assets/font-awesome.min.css">
<link rel="stylesheet" href="/assets/et-line-font.css">
<link rel="stylesheet" href="/assets/themify-icons.css">
<link rel="stylesheet" href="/assets/ace-responsive-menu.css">

<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
<!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
<![endif]-->

</head>
<body class="hold-transition login-page">
<div class="login-box">
  <div class="login-box-body">
    <h3 class="login-box-msg">Sign In</h3>
    
      <div class="form-group has-feedback">
      <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <%= f.email_field :email, class: "form-control sty1", autofocus: true, placeholder: "User" %>
<!--         <input type="email" class="form-control sty1" placeholder="User">
 -->      </div>
      <div class="form-group has-feedback">
      <%= f.password_field :password, class: "form-control sty1", autocomplete: "off", placeholder:"Password"%>
       <!-- <input type="password" class="form-control sty1" placeholder="Password"> -->
      </div>
      <div>
        <div class="col-xs-8">
         <div class="checkbox icheck">
            <label>
              <input type="checkbox">
              Remember Me </label>
             </div>
        </div>
        <!-- /.col -->
        <div class="col-xs-4 m-t-1">
         <%= f.submit "Log in", class: 'btn btn-primary btn-block btn-flat' %>
         <!-- <button type="submit" class="btn btn-primary btn-block btn-flat">Sign In</button> -->
        </div>
        <!-- /.col --> 
      </div>
    </form>
    <!--div class="social-auth-links text-center">
      <p>- OR -</p>
      <a href="#" class="btn btn-block btn-social btn-facebook btn-flat"><i class="fa fa-facebook"></i> Sign in using
      Facebook</a> <a href="#" class="btn btn-block btn-social btn-google btn-flat"><i class="fa fa-google-plus"></i> Sign in using
      Google+</a> </div-->
    <!-- /.social-auth-links -->
    
    <div class="m-t-2">Don't have an account? <%= render "devise/shared/links" %></div>
  </div>
  <%end%>
  <!-- /.login-box-body --> 
</div>
<!-- /.login-box --> 

<!-- jQuery 3 --> 
<script src="/assets/jquery.min.js"></script> 

<!-- v4.0.0-alpha.6 --> 
<script src="/assets/bootstrap.min.js"></script> 

<!-- template --> 
<script src="/assets/niche.js"></script>
</body>
</html>