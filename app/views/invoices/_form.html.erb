      <section class="content">  
        <section class="invoice">  
     <%= nested_form_for @invoice do |f| %>

     
      <form class="form-horizontal">
          
          <div class="row m-d-4">
          <div class="form-group">
           <div class="col">
             <label class="col-sm-10 control-label">Invoice Number</label>   
              <%= f.text_field :invoice_no,class: 'form-control'%>
             </div>
            </div>
            
            
       <div class="form-group">
        <div class="col">
          <label class="col-sm-10 control-label">Date</label>   
          <%= f.date_field :invoice_date,value: Date.today, class: 'form-control'%>
        </div>
      </div> 
     </div>
      

      <div class="form-group">
        <div class="col-sm-3">
          <label class="col-sm-10 control-label">Search Clients</label>   
          <%= f.select :client_id ,Client.all.collect{|i| [i.name, i.id]}, {prompt:true},class: "form-control"%> 
          </div>
      </div>
   
       <div class="form-group">
        <div class="col-sm-3">
          <label class="col-sm-10 control-label">Address</label>   
          <%= text_field_tag :address," ",id: :address, class: "form-control" %>
          </div>
      </div>

      <div class="form-group">
        <div class="col-sm-3">
          <label class="col-sm-10 control-label">Phone No</label>   
         <%= text_field_tag :phone_no," ",id: :phone_no, class: "form-control" %>
          </div>
      </div>

       <div class="form-group">
        <div class="col-sm-3">
          <label class="col-sm-10 control-label">Pan No</label>   
           <%= text_field_tag :pan_no," ",id: :pan_no, class: "form-control" %>
          </div>
      </div>
    

     <h3>Products</h3>

      <div class="panel-body">

        <div class="table-responsive">
          <table id="example"  class="table table-bordered table-striped" cellspacing="0" width="100%">
            <thead>
             <tr>
                    <th width="150"><h6>Name</h6></th>
                    <th width="100"><h6>Description</h6></th>
                     <th width="80"><h6>Price</h6></th>
                     <th width="80"><h6>Cgst</h6></th>
                     <th width="80"><h6>Sgst</h6></th>
                     <th width="80"><h6>Quantity</h6></th>
                     <th width="80"><h6>Total Amt</h6></th>
                     <th width="80"><h6>Action<h6></th>
            </tr>
          </thead>
          
        </table>
        
       <%= f.fields_for :invoice_items do |ing| %>

       <div class="table-responsive">
          <table id="example"  class="table table-bordered table-striped" cellspacing="0" width="100%">
            <colgroup>
            <col class="one"/>
            <col class="two"/>
            <col class="three"/>
            <col class="four"/>
            <col class="five"/>
            <col class="six"/>
            <col class="seven"/>
            <col class="eight"/>
          </colgroup>

          
      <tr class="fields">
       
        <td><%=ing.select :product_id ,Product.all.collect{|i| [i.name, i.id]}, {prompt:true},class: "select"%></td>
        <td> <%= text_field_tag :description," ",id: :description, class: "form-control" %></td>
        <td><%= text_field_tag :price," ",id: :price, class: "form-control" %></td>
        <td><%= text_field_tag :cgst," ",id: :cgst, class: "form-control" %></td>
        <td><%= text_field_tag :sgst," ",id: :sgst, class: "form-control" %></td>
        <td><%=ing.text_field :quantity,id: :quantity, class: 'form-control' %></td>
        <td><%=ing.text_field :total,id: :total, class: 'form-control' %></td>
       <td><%= ing.link_to_remove "Remove", class: 'btn btn-danger btn-flat btn-sm' %></td>
       </tr>
      </table>
     </div>  
   <% end %>
     <p>
            <%= f.link_to_add "Add a Product", :invoice_items, class: 'btn btn-primary btn-flat btn-sm' %>
           <%= f.submit %>
     </p>

   </form>
    <%end%>
