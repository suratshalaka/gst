      <h4 class="text-black">Invoices</h4>
      <div class="table-responsive">
                  <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                        <th>Sr No</th>
                        <th>Client Name</th>
                        <th>Pan No</th>
                        <th>Phone No</th>
                        <th>Date</th>
                        <th>Invoice No</th>
                        <th></th>
                        <th></th>
                      </tr>
                </thead>
                <tbody>
                <% @invoices.each_with_index do |a,i| %>
                      <tr>
                        <td><%= i+1 %></td>
                        <td><%= link_to a.client.name, invoice_path(a), class: 'btn btn-link' %></td>
                        <td><%= a.client.pan_no %></td>
                        <td><%= a.client.phone_no %></td>
                        <td><%= a.invoice_date %></td>
                        <td><%= a.invoice_no %></td>
                        <td><%= link_to 'Edit',edit_invoice_path(a), class: 'btn btn-info btn-sm'%></td>
                        <td><%= link_to 'Destroy',invoice_path(a), method: :delete, data: { confirm: 'Are you sure?' } , class: ' btn btn-danger btn-sm'%></td>
                      </tr>
                     <% end %>
                   </tbody>
                    <p align="left">
                 <%= link_to 'New Invoice', new_invoice_path, class: 'btn btn-warning btn-sm' %> 
                      </p>
               </table>

